class Schedule{
	slot		:	varchar[10],
	owner		:	varchar[25],
	vehicle		:	varchar[30],
	insurance	:	varchar[20],
 	phone_num	:	varchar[15],
 	ro_num		:	unsigned tinyint,
 	reason		:	varchar[15]
}

class Appointment{
	dayDate		:	varchar[20],
	scheds		:	[Schedule]
}


